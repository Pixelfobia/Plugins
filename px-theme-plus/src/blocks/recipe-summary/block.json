{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "px-theme-plus/recipe-summary",
  "title": "Recipe Summary",
  "category": "text",
  "description": "Adds a summary section for a recipe",
  "textdomain": "px-theme-plus",
  "attributes": {
    "prepTime": {
      "type": "string"
    },
    "cookTime": {
      "type": "string"
    },
    "course": {
      "type": "string"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./index.css",
  "usesContext": ["postId"],
  "script": "file:./frontend.js"
}